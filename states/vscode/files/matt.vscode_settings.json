{
    "workbench.colorTheme": "Default Dark+",
    "workbench.iconTheme": "vscode-icons",
    "redhat.telemetry.enabled": false,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.editorAssociations": {
        "git-rebase-todo": "default"
    },
    "yaml.maxItemsComputed": 10000,
    "editor.foldingMaximumRegions": 15000,
    "editor.unicodeHighlight.nonBasicASCII": true,
    "editor.unicodeHighlight.includeComments": true,
    "editor.minimap.maxColumn": 60,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.wrapTabs": true,
    "zenMode.hideTabs": false,
    "workbench.editor.scrollToSwitchTabs": true,
    "workbench.editor.openPositioning": "last",
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.settings.openDefaultSettings": true,
    "workbench.editor.openSideBySideDirection": "down",
    "workbench.editor.labelFormat": "short",
    "workbench.editor.closeOnFileDelete": true,
    "terminal.integrated.cursorWidth": 3,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.scrollback": 10000,
    "telemetry.telemetryLevel": "error",
    "remote.SSH.configFile": "/home/matt/.ssh/config.kettlewell.fam",
    "remote.SSH.showLoginTerminal": true,
    "python.linting.flake8Enabled": true,
    "python.formatting.provider": "black",
    "python.diagnostics.sourceMapsEnabled": true,
    "editor.formatOnSave": true,
    "python.linting.flake8Args": [
        "--max-line-length=100"
    ],
    "python.testing.pytestEnabled": true,
    "python.venvPath": "/data/venv",
    "problems.sortOrder": "position",
    "tabnine.experimentalAutoImports": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "explorer.confirmDelete": false
}
